{
  "private": false,
  "description": "A music app built with Nuxt 3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrDeivi/nuxic"
  },
  "keywords": [
    "Nuxt 3",
    "Music",
    "UnoCSS"
  ],
  "author": "David Alfonso <email@deivi.me>",
  "bugs": {
    "url": "https://github.com/MrDeivi/nuxic/issues"
  },
  "packageManager": "pnpm@8.6.1",

  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev --port=3006",
    "dev:proxy": "cd proxy && pnpm dev",
    "start": "node .output/server/index.mjs",
    "postinstall": "nuxi prepare",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@iconify-json/akar-icons": "^1.1.17",
    "@iconify-json/carbon": "^1.1.17",
    "@iconify-json/cib": "^1.1.4",
    "@iconify-json/iconoir": "^1.1.28",
    "@iconify-json/mingcute": "^1.1.6",
    "@iconify-json/ph": "^1.1.5",
    "@iconify-json/ri": "^1.1.9",
    "@iconify-json/solar": "^1.1.1",
    "@iconify-json/svg-spinners": "^1.1.1",
    "@nuxt/devtools": "^0.5.5",
    "@nuxt/image-edge": "1.0.0-rc.1-28105270.a3967a7",
    "@nuxtjs/color-mode": "^3.2.0",
    "@nuxtjs/device": "^3.1.0",
    "@nuxtjs/google-fonts": "^3.0.1",
    "@nuxtjs/i18n": "8.0.0-beta.12",
    "@pinia/nuxt": "^0.4.11",
    "@unocss/nuxt": "^0.53.1",
    "@vueuse/nuxt": "^10.1.2",
    "aos": "^2.3.4",
    "axios": "^1.4.0",
    "eslint": "^8.42.0",
    "lru-cache": "^9.1.2",
    "nuxt": "^3.5.3",
    "nuxt-headlessui": "^1.1.4",
    "ohash": "^1.1.2",
    "pinia": "^2.1.3",
    "typescript": "^5.1.3",
    "vue-tsc": "^1.6.5"
